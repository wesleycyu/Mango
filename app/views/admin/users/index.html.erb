<h1>Admin Index </h1>
<%= link_to "Create a user", new_admin_user_path %>
<% @users.each do |user| %>
<p>User ID: <%= user.id %> <small>(<%= link_to "edit", edit_admin_user_path(user.id)%>, <%= link_to "delete", admin_user_path(user.id), :method => :delete, :"data-confirm" => "You sure?" %>)</small></p>
<p>Email: <%= user.email %></p>
<p>Full Name: <%= user.full_name %></p>
<hr/>
<% end %>

<%= paginate @users %>