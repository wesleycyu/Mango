<h1>Admin Index </h1>
<%= link_to "Create a user", new_admin_user_path %>
<% @users.each do |user| %>
<div class="user_card">
  <p>User ID: <%= user.id %> <small>(<%= link_to "edit", edit_admin_user_path(user.id)%>, <%= link_to "delete", admin_user_path(user.id), :remote => true, :method => :delete, :class => "delete-user", :"data-confirm" => "You sure?" %>)</small></p>
  <p>Email: <%= link_to user.email, become_path(user) %></p>
  <p>Full Name: <%= user.full_name %></p>
  <hr/>
</div>
<% end %>

<%= paginate @users %>